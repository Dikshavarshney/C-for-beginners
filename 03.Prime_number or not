#include<stdio.h>
int main()
{
 int num,i,count=0;
 printf("Enter number---");
 scanf("%d",&num);
  

printf("The number %d is divided by \n",num);//print all the dividend of num;

for(i=1;i<=num;i++){
     if(num%i==0){//check condition, is true then execute if statement otherwise else statement;
 
            count++;//increment of count;
           printf("%d\n",i);//print the i; 
     }
 }
printf("%d is divisilbe %d times\n", num,count);//print the num as well as count;
if(count==2){//check the comdition, is true execute if statement otherwise esle statement;
 
    printf("so %d is a prime number\n",num);
 }

else{
    printf("so %d is not a prime number\n",num);
}
 return 0;
}
